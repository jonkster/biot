{
  "name": "biotz-web-interface",
  "version": "0.0.0",
  "description": "A web interface for viewing Biotz data",
  "scripts": {
    "typings-install": "typings install",
    "postinstall": "npm run typings-install",
    "build": "webpack --inline --colors --progress --display-error-details --display-cached",
    "watch": "npm run build -- --watch",
    "server": "webpack-dev-server --inline --colors --progress --display-error-details --display-cached --port 3000  --content-base src",
    "start": "npm run server"
  },
  "contributors": [
    "Jon Kelly <jonk@bigpond.com>"
  ],
  "license": "MIT",
  "devDependencies": {
    "angular2-template-loader": "^0.4.0",
    "awesome-typescript-loader": "^1.1.1",
    "css-loader": "^0.23.1",
    "json-loader": "^0.5.4",
    "raw-loader": "^0.5.1",
    "source-map-loader": "^0.1.5",
    "to-string-loader": "^1.1.4",
    "typescript": "~1.8.9",
    "typings": "~1.0.3",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0",
    "webpack-merge": "^0.8.4"
  },
  "dependencies": {
    "@angular/common": "2.0.0-rc.4",
    "@angular/compiler": "2.0.0-rc.4",
    "@angular/core": "2.0.0-rc.4",
    "@angular/http": "2.0.0-rc.4",
    "@angular/platform-browser": "2.0.0-rc.4",
    "@angular/platform-browser-dynamic": "2.0.0-rc.4",
    "@angular/platform-server": "2.0.0-rc.4",
    "@angular/router": "3.0.0-beta.2",
    "core-js": "^2.2.0",
    "ie-shim": "^0.1.0",
    "ng2-bootstrap": "^1.1.1",
    "ng2-select": "^1.0.3",
    "rxjs": "5.0.0-beta.6",
    "three": "^0.78.0",
    "three-text2d": "^0.2.3-rc1",
    "zone.js": "~0.6.12"
  },
  "keywords": [
    "Angular2"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jonkster/biotz-interface.git"
  },
  "bugs": {
    "url": "https://github.com/jonkster/biotz-interface/issues"
  },
  "homepage": "https://github.com/jonkster/biotz-interface#readme"
}
